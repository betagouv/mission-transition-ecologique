# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
# Change these settings to your own preference
indent_style = space
indent_size = 2
max_line_length = 140

[*.md]
max_line_length = off
trim_trailing_whitespace = false
