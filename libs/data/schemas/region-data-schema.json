{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Liste de communes avec régions françaises",
  "description": "Un tableau de communes, chaque commune ayant des informations sur son code, nom, code postal, département, et région.",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "codesPostaux": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "nom": {
        "type": "string"
      },
      "code": {
        "type": "string",
        "description": "Code INSEE de la commune"
      },
      "departement": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "description": "Code du département"
          },
          "nom": {
            "type": "string",
            "description": "Nom du département"
          }
        },
        "required": ["code", "nom"]
      },
      "region": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "description": "Code de la région"
          },
          "nom": {
            "type": "string",
            "description": "Nom de la région",
            "enum": [
              "Île-de-France",
              "Centre-Val de Loire",
              "Saint-Pierre-Et-Miquelon",
              "Bourgogne-Franche-Comté",
              "Normandie",
              "Saint-Barthélemy",
              "Hauts-de-France",
              "Wallis et Futuna",
              "Terres australes et antarctiques françaises",
              "Grand Est",
              "Nouvelle-Calédonie",
              "Saint-Martin",
              "Île de Clipperton",
              "Polynésie française",
              "Pays de la Loire",
              "Bretagne",
              "Nouvelle-Aquitaine",
              "Occitanie",
              "Auvergne-Rhône-Alpes",
              "Provence-Alpes-Côte d'Azur",
              "Corse",
              "Guadeloupe",
              "Martinique",
              "Guyane",
              "La Réunion",
              "Mayotte"
            ]
          }
        },
        "required": ["code", "nom"]
      }
    },
    "required": ["codesPostaux", "nom", "code", "departement", "region"]
  }
}
