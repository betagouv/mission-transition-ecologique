{
  "name": "@tee/data",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project @tee/data --web",
  "targets": {
    "generate-program": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/data",
        "commands": [
          "npx ts-node src/generateProgramType.ts",
          "npx ts-node src/buildProgramJson.ts"
        ],
        "parallel": false
      }
    },"generate-projects": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/data",
        "commands": [
          "npx ts-node src/buildProjectsJson.ts"
        ]
      }
    }
  }
}
