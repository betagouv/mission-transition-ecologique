    "ci": "nx run-many -t ci --verbose --parallel=false",
    "sass:watch": "nx run @tee/nuxt:sass-watch",
    "e2e": "VITE_DATA_TEST=true PLAYWRIGHT_HTML_OPEN=never nx run @tee/nuxt-e2e:e2e"
