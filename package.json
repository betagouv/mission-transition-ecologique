  "type": "module",
  "scripts": {
    "dev": "nx run-many -t serve",
    "dev:start": "PORT=4242 npm run start",
    "dev:build:start": "npm run build && npm run dev:start",
    "build": "export NODE_OPTIONS=--max-old-space-size=6144 && nx run-many -t build",
    "start": "node apps/nuxt/.output/server/index.mjs --port $PORT",
    "build:etl": "nx run @tee/etl:build-docker",
    "build:start": "npm run build && npm run start",
    "generate:programs": "nx run @tee/data:generate-programs",
    "generate:projects": "nx run @tee/data:generate-projects",
    "generate:iframe-script": "nx run @tee/nuxt:generate-iframe-script",
    "generate:iframe-script:watch": "nx run @tee/nuxt:generate-iframe-script --watch",
    "database:update": "nx run @tee/etl:update-database",
    "test": "nx run-many -t test -p @tee/data @tee/backend-ddd --verbose",
    "lint": "nx run-many -t lint --verbose",
    "lint:fix": "nx run-many -t lint --fix --verbose",
    "lint:sass": "nx run @tee/nuxt:sass-lint",
    "lint:sass:fix": "nx run @tee/nuxt:sass-lint --fix",
    "type:check": "nx run-many -t type-check --verbose --parallel=false",
    "ci": "nx run-many -t ci --verbose --parallel=false",
    "sass:watch": "nx run @tee/nuxt:sass-watch",
    "e2e": "VITE_DATA_TEST=true && THIRD_PARTY_API_ENABLED=false && npm run build && nx run @tee/nuxt-e2e:e2e",
    "e2e-ui": "npm run e2e -- --ui",
    "docker:python": "nx run @tee/etl:docker-python",
    "cleanup": "nx run @tee/nuxt:cleanup"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=11.0 <11"
  },
